; Simple three dimensional cube
; OMEGOS 3d format

Object(4)
{
	AddVertex(200000, 0, -200000)
	AddVertex(200000, 0, 200000)
	AddVertex(-200000, 0, 200000)
	AddVertex(-200000, 0, -200000)

	AddLambertPolygon(4,3,2,1,0)
	{
		SetColor(0,128,0)
	}
	AddLambertPolygon(4,0,1,2,3)
	{
		SetColor(0,120,0)
	}
}
